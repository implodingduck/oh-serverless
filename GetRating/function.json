{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "ratings/{ratingId}",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "name": "doc",
      "databaseName": "bfyocproductrating",
      "collectionName": "ProductRating",
      "connectionStringSetting": "AzureCosmosDBConnectionString",
      "direction": "in",
      "id": "{ratingId}"
    }
  ]
}